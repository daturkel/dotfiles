{
    "pairs.enableCharacters":["(", "[", "{", "'", "\"", "`"],
    "diagnostic.errorSign": "X",
    "diagnostic.warningSign": "!",
    "diagnostic.infoSign": "I",
    "python.formatting.provider": "black",
    "python.linting.enabled": false,
    "coc.source.around.disableSyntaxes": ["comment","string"],
    "coc.source.buffer.disableSyntaxes": ["comment","string"],
    "suggest.autoTrigger": "none",
    "suggest.minTriggerInputLength": 3,
    "suggest.noselect": false,
    "python.analysis.typeCheckingMode": "off",
    "pyright.disableDiagnostics": true,
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/"],
            "filetypes": ["go"],
            "initializationOptions": 
                {"usePlaceholders": true}
        }
    }
}
